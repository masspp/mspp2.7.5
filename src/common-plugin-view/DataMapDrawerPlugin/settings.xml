<?xml version="1.0"?>

<settings
    xmlns="http://xml.first-ms3d.jp/masspp/settings"  
    xsi:schemaLocation="http://xml.first-ms3d.jp/masspp/settings http://first-ms3d.jp/development/masspp-settings110.xsd"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <chapters>
    <chapter id="0">View</chapter>
  </chapters>
  <values>
    <value id="0" type="string" default="grayscale" required="true">
      <enum>
        <enum-name>Grayscale</enum-name>
        <enum-value>grayscale</enum-value>
      </enum>
      <enum>
        <enum-name>Thermography</enum-name>
        <enum-value>thermography</enum-value>
      </enum>
      <enum>
        <enum-name>Flame</enum-name>
        <enum-value>flame</enum-value>
      </enum>
      <enum>
        <enum-name>French</enum-name>
        <enum-value>french</enum-value>
      </enum>
      <enum>
        <enum-name>Deep Sea</enum-name>
        <enum-value>deepsea</enum-value>
      </enum>
    </value>
    <value id="1" type="string" default="linear" required="true">
      <enum>
        <enum-name>Linear</enum-name>
        <enum-value>linear</enum-value>
      </enum>
      <enum>
        <enum-name>Square</enum-name>
        <enum-value>square</enum-value>
      </enum>
      <enum>
        <enum-name>Log</enum-name>
        <enum-value>log</enum-value>
      </enum>
    </value>
  </values>
  <params>
    <param id="0" value="0">
      <param-name>theme</param-name>
      <param-section>Data Map Draw</param-section>
      <param-key>COLOR_THEME</param-key>
    </param>
    <param id="1" value="1">
      <param-name>scale</param-name>
      <param-section>Data Map Draw</param-section>
      <param-key>DRAW_SCALE</param-key>
    </param>
  </params>
  <pages>
    <page chapter="0" common="true">
      <page-name>common</page-name>
      <page-title>Heatmap</page-title>
      <forms>
        <form id="0" param="0">
          <form-title>Theme</form-title>
        </form>
        <form id="1" param="1">
          <form-title>Scale</form-title>
        </form>
      </forms>
    </page>

    <page chapter="0" common="true">
      <page-name>heatmap</page-name>
      <page-title>Heatmap</page-title>
      <properties-page type="heatmap_view" />
      <forms>
        <form id="0" param="0">
          <form-title>Theme</form-title>
        </form>
        <form id="1" param="1">
          <form-title>Scale</form-title>
        </form>
      </forms>
    </page>

  </pages>
</settings>
