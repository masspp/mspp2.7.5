        <form id="3" param="0">
          <form-title>Window Width</form-title>
          <form-desc img="images/abc-baseline-0.png">Set the "window width" of ABC baseline detection method.</form-desc>
        </form>
        <form id="4" param="1">
          <form-title>Noise Factor</form-title>
          <form-desc img="images/abc-baseline-1.png">Set the "noise factor" of ABC baseline detection method.</form-desc>
        </form>
        <form id="5" param="2">
          <form-title>Smoothing Width</form-title>
          <form-desc img="images/abc-baseline-2.png">Set the "smoothing width" of ABC baseline detection method.</form-desc>
