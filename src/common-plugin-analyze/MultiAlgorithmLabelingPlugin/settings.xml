      <param-name>centroid_mode</param-name>
      <param-section>Multi Algorithm Labeling</param-section>
      <param-key>CENTROID_PEAK_DETECTOR</param-key>
    </param>
  </params>
  <pages>
    <page common="false">
      <page-name>Spectrum Peak Detection</page-name>
      <page-title>Multi Algorithm</page-title>
      <forms>
        <form id="0" param="0">
          <form-title>Profile Mode</form-title>
          <form-desc>Select the peak detection method for profile mode spectra.</form-desc>
        </form>
        <form id="1" param="1">
          <form-title>Centroid Mode</form-title>
          <form-desc>Select the peak detection method for centroid mode spectra.</form-desc>
        </form>
      </forms>
    </page>
  </pages>
</settings>

